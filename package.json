{
  "name": "moderu",
  "version": "0.5.4",
  "description": "Type-safe library for knex query & model.",
  "main": "build/src/index.js",
  "scripts": {
    "compile": "tsc",
    "bundle": "dts-bundle --name moderu --main build/src/index.d.ts",
    "watch": "tsc --watch",
    "unit": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail test",
    "unit:prebuilt": "NODE_ENV=test mocha --recursive --bail build/test",
    "cover": "NODE_ENV=test nyc npm run unit",
    "cover:check": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "build": "npm run compile && npm run bundle",
    "clean": "rm -rf build && rm -rf coverage && rm -rf .nyc_output",
    "prepublish": "npm run clean && npm run build && npm test",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "test": "npm run lint && npm run cover",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish"
  },
  "homepage": "https://github.com/vietthang/moderu",
  "author": "Yoshi",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/knex": "^0.0.50",
    "@types/mocha": "^2.2.41",
    "@types/node": "7.0.5",
    "dts-bundle": "^0.7.2",
    "istanbul": "^0.4.5",
    "knex": "^0.12.7",
    "mocha": "^3.2.0",
    "mock-knex": "^0.3.7",
    "mysql": "^2.13.0",
    "nyc": "^10.1.2",
    "source-map-support": "^0.4.11",
    "sqlite3": "^3.1.8",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "sukima": "^0.13.0"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "typings": "build/src/moderu.d.ts"
}
