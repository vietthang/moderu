{
  "name": "moderu",
  "version": "0.0.4",
  "description": "Type-safe library for knex query & model.",
  "main": "build/src/index.js",
  "scripts": {
    "compile": "tsc",
    "bundle": "dts-bundle --name moderu --main build/src/index.d.ts",
    "test": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail",
    "cover": "NODE_ENV=test nyc npm test",
    "build": "npm run compile && npm run bundle",
    "clean": "rm -rf build",
    "prepublish": "npm run clean && npm run build"
  },
  "homepage": "https://github.com/vietthang/moderu",
  "author": "Yoshi",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/chai": "^3.4.34",
    "@types/knex": "0.0.39",
    "@types/mocha": "^2.2.38",
    "@types/node": "7.0.0",
    "chai": "^3.5.0",
    "dts-bundle": "^0.6.1",
    "istanbul": "^0.4.5",
    "knex": "^0.12.6",
    "mocha": "^3.2.0",
    "mysql": "^2.12.0",
    "nyc": "^10.1.2",
    "source-map-support": "^0.4.10",
    "sqlite3": "^3.1.8",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "^2.1.5"
  },
  "dependencies": {
    "sukima": "^0.5.5"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "typings": "build/src/moderu.d.ts"
}
