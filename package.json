{
  "name": "moderu",
  "version": "0.1.4",
  "description": "Type-safe library for knex query & model.",
  "main": "build/src/index.js",
  "scripts": {
    "compile": "tsc",
    "bundle": "dts-bundle --name moderu --main build/src/index.d.ts",
    "test": "NODE_ENV=test mocha --recursive --compilers ts:ts-node/register --bail",
    "cover": "NODE_ENV=test nyc npm test",
    "build": "npm run compile && npm run bundle",
    "clean": "rm -rf build",
    "prepublish": "npm run clean && npm run build"
  },
  "homepage": "https://github.com/vietthang/moderu",
  "author": "Yoshi",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/chai": "^3.4.35",
    "@types/knex": "0.0.41",
    "@types/mocha": "^2.2.39",
    "@types/node": "7.0.5",
    "chai": "^3.5.0",
    "dts-bundle": "^0.7.2",
    "istanbul": "^0.4.5",
    "knex": "^0.12.7",
    "mocha": "^3.2.0",
    "mysql": "^2.13.0",
    "nyc": "^10.1.2",
    "source-map-support": "^0.4.11",
    "sqlite3": "^3.1.8",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "sukima": "^0.6.2"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "typings": "build/src/moderu.d.ts"
}
